  },
  "author": "Vlad Balin <https://github.com/gaperton>",
  "contributors": [],
  "peerDependencies": {
    "jquery": "*",
    "underscore": "*"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.48",
    "chai": "* <3",
    "chai-as-promised": "^5.3.0",
    "jquery": "*",
    "mocha": "^3.5.3",
    "requirejs": "^2.3.5",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^2.3.7",
    "typescript": "<2.4.0",
    "underscore": "*",
    "webpack": "^3.5.6"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "LICENSE"
  ],
  "license": "MIT",
  "scripts": {
    "test": "node_modules/.bin/mocha",
